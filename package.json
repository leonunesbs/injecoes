{
  "name": "antivegf",
  "description": "AntiVEGF é uma plataforma restrita ao serviço de oftalmologia do Hospital Geral de Fortaleza (HGF), projetada para auxiliar os médicos residentes em suas atividades diárias, permitindo a organização de relatórios de injeções intravítreas de forma prática e rápida.",
  "version": "1.0.0",
  "private": true,
  "author": "Leonardo Nunes <leonunesbs@gmail.com> (@leonunesbs)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "prisma generate && husky"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@prisma/client": "5.22.0",
    "@vercel/blob": "0.25.1",
    "next": "14.2.17",
    "papaparse": "5.4.1",
    "pdf-lib": "1.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.2",
    "react-icons": "5.3.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@tailwindcss/typography": "0.5.15",
    "@types/file-saver": "2.0.7",
    "@types/node": "20.17.6",
    "@types/nodemailer": "6.4.16",
    "@types/papaparse": "5.3.14",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "10.4.20",
    "daisyui": "4.12.14",
    "date-fns": "4.1.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.17",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.10",
    "postcss": "8.4.47",
    "prettier": "3.2.5",
    "prettier-plugin-sort-json": "4.0.0",
    "prisma": "5.22.0",
    "tailwindcss": "3.4.14",
    "typescript": "5.4.5",
    "zod": "3.23.8"
  }
}
